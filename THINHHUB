-- Báº¢O Máº¬T KEY (THINH?)
local HIDDEN_HASH = "84|72|73|78|72|63"
local function CheckPass(str)
    local code = ""
    for i = 1, #str do code = code .. string.byte(str, i, i) .. "|" end
    return code:sub(1, -2) == HIDDEN_HASH
end

